#include<string>
#define ll long long
#define vl vector<ll>
#define vi vector<int>
#define v2 vector<vector<int>>
#define pb push_back
#define nl endl
#define yes "YES"
#define no "NO"
#define mod 1000000007
#define mod1 998244353
#define ull unsigned long long
#define     in_range(i,x,y)    for(int i=x;i<y;i++)
#define     in_range1(i,x,y)    for(int i=x;i<=y;i++)
#define st_input for(int i=x; i<y; i++) {cin>>st[i];}
#define  N 1000
using namespace std;

void solve(){
    ll x,y,m,ans,n,i,j;
    cin>>n>>m>>x;
    if(n%m==0){
        cout<<-1<<nl;
        return;
    }
    else{
        ans = (x/n + 1)*n;
        if(x%n ==0 ) ans=x;
        if(ans%m==0) ans+=n;
        cout<<ans<<nl;
    }

}
int main(){
    ll i,tc,n,Pow,m,ans;
    cin>>tc;
    while(tc--){
        solve();
    }
return 0;
}

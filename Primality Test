////Codechef Primality Test time complexity O(sqrt(n))

#include<bits/stdc++.h>
#include<math.h>
#include<string>
#define ll long long
#define vl vector<ll>
#define vi vector<int>
#define v2 vector<vector<int>>
#define pb push_back
#define nl endl
#define yes "YES"
#define no "NO"
#define mod 1000000007
#define mod1 998244353
#define mx  1e69
#define ull unsigned long long
#define     in_range(i,x,y)    for(int i=x;i<y;i++)
#define     in_range1(i,x,y)    for(int i=x;i<=y;i++)
#define st_input for(int i=x; i<y; i++) {cin>>st[i];}
using namespace std;

void solve(){
    ll i,j,n,y,z,c,m,x,a,b;
    cin>>n;
    if(n==1){
        cout<<"no"<<nl;
    }
    else{
        bool flag=true;
        for(i=2; i*i<=n; i++){
            if(n%i==0){
                flag=false;
                break;
            }
        }
        if(flag==false)cout<<"no"<<nl;
        else cout<<"yes"<<nl;
    }

}
int main(){
    ll i,tc,n,ans;
    cin>>tc;
    while(tc--){
       solve();
    }
return 0;
}

///TIME COMPLEXITY O(logn)


#include<bits/stdc++.h>
#include<math.h>
#include<string>
#define ll long long
#define vl vector<ll>
#define vi vector<int>
#define v2 vector<vector<int>>
#define pb push_back
#define nl endl
#define yes "YES"
#define no "NO"
#define mod 1000000007
#define mod1 998244353
#define mx  1e69
#define ull unsigned long long
#define     in_range(i,x,y)    for(int i=x;i<y;i++)
#define     in_range1(i,x,y)    for(int i=x;i<=y;i++)
#define st_input for(int i=x; i<y; i++) {cin>>st[i];}
using namespace std;

ll solve(ll base,ll Pow){
    ll res=1;
    while(base){
        if(Pow%2!=0){
            res*=base;
            Pow--;
        }
        else{
            base*=base;
            Pow/=2;
        }
    }
    return res;
}
int main(){
    ll i,tc,n,Pow,ans;
    //in>>tc;
    cin>>n>>Pow;
    //while(tc--){
        cout<<solve(n,Pow)<<nl;
    //}
return 0;
}


*******MOD Pow*****
#include<bits/stdc++.h>
#include<math.h>
#include<string>
#define ll long long
#define vl vector<ll>
#define vi vector<int>
#define v2 vector<vector<int>>
#define pb push_back
#define nl endl
#define yes "YES"
#define no "NO"
#define mod 1000000007
#define mod1 998244353
#define mx  1e69
#define ull unsigned long long
#define     in_range(i,x,y)    for(int i=x;i<y;i++)
#define     in_range1(i,x,y)    for(int i=x;i<=y;i++)
#define st_input for(int i=x; i<y; i++) {cin>>st[i];}
using namespace std;

ll solve(ll base,ll Pow){
    ll res=1;
    while(Pow){
        if(Pow%2!=0){
            res=(res*base)%mod;
            Pow--;
        }
        else{
            base=(base*base)%mod;
            Pow/=2;
        }
    }
    return res;
}
int main(){
    ll i,tc,n,Pow,ans;
    //in>>tc;
    cin>>n>>Pow;
    //while(tc--){
        cout<<solve(n,Pow)<<nl;
    //}
return 0;
}

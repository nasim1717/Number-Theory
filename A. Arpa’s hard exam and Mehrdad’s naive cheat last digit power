int cas=0;
ll Pow(ll a, ll n, ll m){
    ll res=1;
    while(n){
        if(n&1){
            res = (res*a)%m;
            n--;
        }
        else{
            a = (a*a)%m;
            n/=2;
        }
    }
    return res;
}
void solve(){
    ll n;
     cin>>n;
    ll ans =  Pow(8,n,10);
    cout<<ans<<nl;
}

